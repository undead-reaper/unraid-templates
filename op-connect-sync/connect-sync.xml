  <WebUI/>
  <TemplateURL/>
  <Icon>https://github.com/undead-reaper/unraid-templates/blob/master/connect-sync/connect-sync.png?raw=true</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1757230295</DateInstalled>
  <DonateText/>
  <DonateLink/>
