<?xml version="1.0"?>
<Container version="2">
  <Name>Quickdrop</Name>
  <Repository>roastslav/quickdrop</Repository>
  <Registry>https://registry.hub.docker.com/r/roastslav/quickdrop</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/RoastSlav/quickdrop</Project>
  <Overview>QuickDrop is an easy-to-use file sharing application that allows users to upload files without an account, generate download links, and manage file availability, file encryption and optional password protection.</Overview>
  <Category>Productivity: Tools:</Category>
  <WebUI>http://[IP]:[PORT:8080]</WebUI>
  <TemplateURL/>
  <Icon/>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1734032131</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Path: /app/db" Target="/app/db" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="Path: /app/log" Target="/app/log" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="Path: /files" Target="/files" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="Port: WebUI" Target="8080" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"/>
</Container>